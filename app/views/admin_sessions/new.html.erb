<%= provide(:title, 'Admin Login')%>
  
<%= form_for @admin_session, :url => admin_session_path do |f| %>  <h1>TEST</h1>
  <p>  
    <%= f.label :login %><br />  
    <%= f.text_field :login %>  
  </p>  
  
  <p>  
    <%= f.label :password %><br />  
    <%= f.password_field :password %>  
  </p>  
  
  <%= f.submit "Login" %>  
<% end %>  